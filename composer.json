{
    "name": "olssonm/swedish-entity",
    "description": "Validator and formatter for Swedish personnummer and organisationsnummer. With Laravel validators.",
    "keywords": [
        "Laravel",
        "package",
        "olssonm",
        "swedish",
        "personal identity number",
        "social security number",
        "personnummer",
        "organizational number",
        "organisationsnummer"
    ],
    "homepage": "https://github.com/olssonm/swedish-entity",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Olsson",
            "email": "contact@marcusolsson.me",
            "homepage": "https://marcusolsson.me"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "personnummer/personnummer": "^3.0"
    },
    "suggest": {
        "laravel/framework": "For Laravel-integration"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Olssonm\\SwedishEntity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olssonm\\SwedishEntity\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpsniff": "vendor/bin/phpcs --standard=\"PSR12\" ./src"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Olssonm\\SwedishEntity\\SwedishEntityServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
